{
	"name": "the-brownie-blog",
	"private": true,
	"description": "My blog",
	"version": "0.0.1",
	"author": "Andria Brown <andria_girl@proton.me>",
	"bugs": {
		"url": "https://github.com/andria-dev/brownie/issues"
	},
	"dependencies": {
		"@apollo/client": "^3.7.3",
		"@apollo/server": "^4.3.0",
		"apollo-server-nextjs": "4.0.0",
		"chokidar": "^3.4.3",
		"esm": "^3.2.25",
		"graphql": "^16.6.0",
		"graphql-tag": "^2.12.6",
		"graphql-tools": "^8.3.14",
		"htm": "^3.0.4",
		"ink": "^3.0.8",
		"mini-debounce": "^1.0.8",
		"minimist": "^1.2.5",
		"next": "^13.1.1",
		"prism-themes": "^1.4.0",
		"prismjs": "^1.20.0",
		"prop-types": "^15.7.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-typography": "^0.16.23",
		"reading-time": "^1.2.0",
		"rehype-parse": "^7.0.1",
		"rehype-stringify": "^8.0.0",
		"remark-extract-frontmatter": "^3.0.0",
		"remark-frontmatter": "^3.0.0",
		"remark-html": "^13.0.1",
		"remark-parse": "^9.0.0",
		"remark-prism": "^1.1.17",
		"remark-rehype": "^8.0.0",
		"remark-stringify": "^9.0.0",
		"styled-components": "^5.2.1",
		"typeface-merriweather": "^0.0.72",
		"typeface-montserrat": "^0.0.75",
		"typography": "^0.16.19",
		"typography-theme-wordpress-2016": "^0.16.19",
		"unified": "^9.2.0",
		"unist-builder": "^2.0.3",
		"unist-util-filter": "^2.0.3",
		"unist-util-map": "^2.0.1",
		"unist-util-visit": "^2.0.3",
		"yaml": "^1.10.0"
	},
	"devDependencies": {
		"husky": "^4.2.5",
		"lint-staged": "^10.1.6",
		"prettier": "^2.0.4"
	},
	"homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
	},
	"scripts": {
		"develop": "next",
		"build": "export NODE_ENV=production && yarn run build:posts && yarn run build:pages",
		"build:pages": "next build",
		"build:posts": "node -r esm ./markdown-compiler.js",
		"watch:posts": "node -r esm ./markdown-compiler.js --watch",
		"format": "prettier --write components/**/*.{js,jsx,json,md} pages/**/*.{js,jsx,json,md} utils/**/*.{js,jsx,json,md} public/**/*.{js,jsx,json,md}",
		"test": "echo \"Write tests! -> https://medium.com/frontend-digest/setting-up-testing-library-with-nextjs-a9702cbde32d\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx}": "prettier --write"
	},
	"engines": {
		"node": "18.x"
	},
	"volta": {
		"node": "18.12.1"
	}
}
